@prefix tm:  <http://www.example.org/turing-machine#> .

# inspired by https://turingmachinesimulator.com/

[] tm:hasCurrentTapeSymbol "0" ;
     tm:hasCurrentState <q0> ;
     tm:hasNextState <q0> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "0" .

[] tm:hasCurrentTapeSymbol "1" ;
     tm:hasCurrentState <q0> ;
     tm:hasNextState <q1> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "1" .

[] tm:hasCurrentTapeSymbol "0" ;
     tm:hasCurrentState <q1> ;
     tm:hasNextState <q2> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "0" .

[] tm:hasCurrentTapeSymbol "1" ;
     tm:hasCurrentState <q1> ;
     tm:hasNextState <q0> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "1" .

[] tm:hasCurrentTapeSymbol "0" ;
     tm:hasCurrentState <q2> ;
     tm:hasNextState <q1> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "0" .

[] tm:hasCurrentTapeSymbol "1" ;
     tm:hasCurrentState <q2> ;
     tm:hasNextState <q2> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "1" .

[] tm:hasCurrentTapeSymbol "#" ;
     tm:hasCurrentState <q0> ;
     tm:hasNextState <Accept> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "#" .

[] tm:hasCurrentTapeSymbol "#" ;
     tm:hasCurrentState <q1> ;
     tm:hasNextState <RemainderOf1> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "#" .

[] tm:hasCurrentTapeSymbol "#" ;
     tm:hasCurrentState <q2> ;
     tm:hasNextState <RemainderOf2> ;
     tm:hasDisplacement tm:R ;
     tm:hasWriteSymbol "#" .

